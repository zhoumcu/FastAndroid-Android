    <FrameLayout
        android:id="@+id/contains"
        android:layout_marginTop="45dp"
        android:layout_marginBottom="49dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <include layout="@layout/bottom_main" />
</RelativeLayout>